{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended"
    ],
    "baseBranches": [
        "main"
    ],
    "packageRules": [
        {
            "autoMerge": true,
            "matchPackagePatterns": [
                ".*"
            ],
            "rangeStrategy": "bumpMinor"
        }
    ]
}